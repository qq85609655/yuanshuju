body {
    padding:20px;
    padding-top:32px;
    font-family:"微软雅黑"!important;
}
.x-body {
    font-family:"微软雅黑"!important;
    font-size:13px;
}
p {
    margin-bottom:15px;
}
h1 {
    font-size:18px;
    margin-bottom:20px;
}
h2 {
    font-size:14px;
    color:#333;
    font-weight:bold;
    margin:10px 0;
}
.example-info{
    width:150px;
    border:1px solid #c3daf9;
    border-top:1px solid #DCEAFB;
    border-left:1px solid #DCEAFB;
    background:#ecf5fe url( info-bg.gif ) repeat-x;
    font-size:10px;
    padding:8px;
}
pre.code{
    background: #F8F8F8;
    border: 1px solid #e8e8e8;
    padding:10px;
    margin:10px;
    margin-left:0px;
    border-left:5px solid #e8e8e8;
    font-size: 12px !important;
    line-height:14px !important;
}
.msg .x-box-mc {
    font-size:14px;
}
#msg-div {
    position:absolute;
    left:42%;
    top:10px;
    width:300px;
    z-index:20000;
    background: #ff0000;
}
#msg-div .msg {
    border-radius: 8px;
    -moz-border-radius: 8px;
    background: #F6F6F6;
    border: 2px solid #ccc;
    margin-top: 2px;
    padding: 10px 15px;
    color: #555;
}
#msg-div .msg h3 {
    margin: 0 0 8px;
    font-weight: bold;
    font-size: 15px;
}
#msg-div .msg p {
    margin: 0;
}
.x-grid3-row-body p {
    margin:5px 5px 10px 5px !important;
}

.feature-list {
    margin-bottom: 15px;
}
.feature-list li {
    list-style: disc;
    margin-left: 17px;
    margin-bottom: 4px;
}
.ul-topmenu li a{ color:#fff !important;}
#panel-1014{ color:#fff}
#treeview-1049{background:#364050 }/*#4577A5*/
#treeview-1049 .x-tree-node-text{ color:#fff; font-size:14px; font-family:"微软雅黑"}
#treeview-1049 .x-grid-cell{background: #364050}
#treeview-1049 .x-grid-cell-inner-treecolumn{padding:15px 6px 10px 0;  border-bottom:1px solid #364050; border-top:1px solid #364050;}
#treeview-1049 .x-grid-cell-inner-treecolumn:visited { background: #36c7d3}
.x-grid-row-before-over{ background:#1e3e5b}
.li-selected{ background:#36c7d3!important;}
.ul-topmenu li a{ padding-top:45px!important}
.x-grid-cell,.x-btn-default-toolbar-small .x-btn-inner,.x-panel-header-text-container-default,.x-column-header,.x-toolbar-item .x-form-cb-label{ font-family:"微软雅黑"!important}
#treeview-1049 .x-grid-row-focused .x-grid-td{ border:0}
#treeview-1049 .x-grid-cell{ border:0}
#treeview-1049 .x-grid-row-before-focused .x-grid-td{ border:0}
#treeview-1049 .x-grid-row-focused .x-grid-td{ background:#36c7d3;  }
#panel-1019  .x-panel-header-default-top{background:#386289; color:#fff; padding:10px; border:0; border-bottom:1px solid #364050!important;}
#panel-1019  .x-panel-header-default-top .x-header-text{ color:#fff; font-size:14px;  box-shadow:0 0 0px 0 0 inset}
#panel-1019 .x-panel-body-default{ border:0}
#panel-1019 .x-grid-header-ct{ border-color:#364050}
#panel-1019 .x-grid-body,#panel-1019 .x-panel-default{ border:0}
#panel-1019  .x-toolbar-default{ background:#364050; color:#fff}
#panel-1019 .x-btn-default-toolbar-small .x-btn-inner{ color:#fff}
#panel-1019 .x-toolbar-default{ border:0}

 