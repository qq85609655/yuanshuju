<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>gilight.dep.metadata</groupId>
	<artifactId>dep-metadata-gather-dbsource</artifactId>
	<version>2.0.0</version>
	<dependencies>
		<dependency>
			<groupId>gilight.dep.framework</groupId>
			<artifactId>gilightframework-dao-db</artifactId>
			<version>2.0.0</version>
		</dependency>
		<dependency>
			<groupId>gilight.dep.metadata</groupId>
			<artifactId>dep-metadata-gather-common</artifactId>
			<version>2.0.0</version>
		</dependency>
		<dependency>
			<groupId>gilight.dep.metadata</groupId>
			<artifactId>dep-metadata-repository</artifactId>
			<version>2.0.0</version>
		</dependency>
		<!-- https://mvnrepository.com/artifact/org.apache.metamodel/MetaModel-full -->
		<dependency>
			<groupId>org.apache.metamodel</groupId>
			<artifactId>MetaModel-full</artifactId>
			<version>4.6.0</version>
		</dependency>
		<dependency>
			<groupId>com.fasterxml.jackson.core</groupId>
			<artifactId>jackson-annotations</artifactId>
			<version>2.7.5</version>
		</dependency>
	</dependencies>
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-jar-plugin</artifactId>
				<version>2.3.1</version>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.7</source>
					<target>1.7</target>
				</configuration>
			</plugin>
		</plugins>

	</build>
</project>