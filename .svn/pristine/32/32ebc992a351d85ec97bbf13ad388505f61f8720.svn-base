	@Override
	public IResponse delMapping(IContext context) throws ServiceException {
		String depeId=(String) context.getParameter("id");
		MdMetadata mdMetadata = new MdMetadata();
		mdMetadata.setId(depeId);
		depDepeMappingModel.setDepe(mdMetadata);
		if (depDepeMappingModel.delMapping()){
			context.getResponse().setResult(
					ResultFactory.getInstance().createSuccessModelAndView(
							"删除成功!", null));
		}else {
			context.getResponse().setResult(
					ResultFactory.getInstance().createSuccessModelAndView(
							"删除失败!", null));
		}
		return context.getResponse();
	}


