	private List<MappDepeView> mapping2Views(List<DepDepeMappingModel> depDepeMappingModels){
		List<MappDepeView> list=new ArrayList<MappDepeView>();
		if (depDepeMappingModels==null||depDepeMappingModels.size()==0){
			return list;
		}
		for (DepDepeMappingModel depDepeMappingModel:depDepeMappingModels) {
			MappDepeView mappDepeView = new MappDepeView();
			MdMetadata sourceMd=depDepeMappingModel.getSource();
			MdMetadata targetMd=depDepeMappingModel.getTarget();
			MdMetadata depeMd=depDepeMappingModel.getDepe();

			mappDepeView.setSourceId(sourceMd.getId());
			mappDepeView.setSourceCode(sourceMd.getMdCode());
			mappDepeView.setSourceName(sourceMd.getMdName());

			mappDepeView.setMappCode(depeMd.getMdCode());
			mappDepeView.setMappId(depeMd.getId());
			mappDepeView.setMappName(depeMd.getMdName());

			mappDepeView.setTargetCode(targetMd.getMdCode());
			mappDepeView.setTargetId(targetMd.getId());
			mappDepeView.setTargetName(targetMd.getMdName());
			mappDepeView.setMappTypeId(depeMd.getMmId());
			MmMetamodel mmMetamodel = new MmMetamodel();
			mmMetamodel.setId(depeMd.getMmId());
			mmMetaModelModel.setMmMetamodel(mmMetamodel);
			mmMetamodel=mmMetaModelModel.queryMmMetamodelById();
			mappDepeView.setMappTypeName(mmMetamodel.getMmName());
			//String mmNmae=
			//mappDepeView.setMappTypeName();
			//mappDepeView.setMappTypeName();

			list.add(mappDepeView);
		}
		return list;
	}

	@Override
	public IResponse queryMapping(IContext context) throws ServiceException {
		List<MappDepeView> list=new ArrayList<MappDepeView>();

		try {
			String mdId=(String) context.getParameter("id");
			MdMetadata mdMetadata = new MdMetadata();
			mdMetadata.setId(mdId);
			depDepeMappingModel.setSource(mdMetadata);
			List<DepDepeMappingModel> depDepeMappingModels=depDepeMappingModel.queryMapping();

			list= mapping2Views(depDepeMappingModels);
		} catch (ModelException e) {
			e.printStackTrace();
			logger.error("查询映射失败",e);
		}
		context.getResponse().setResult(
				ResultFactory.getInstance().createSuccessModelAndView(
						"获取成功!", list));
		return context.getResponse();
	}

